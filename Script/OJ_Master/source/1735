
//Problem1735


#include<stdio.h>
int main()

{
	int n;
	scanf("%d", &n);
	int a[10000];
	int i,k;
	for (i = 0; i < n; i++)
	{
		scanf("%d", &a[i]);
	}
	for (k = 0; k < i; k++)
	{
		if (a[k] < 0)
			printf("%d ",a[k]);
	}
	for (k = 0; k < i; k++)
	{
		if (a[k] == 0)
			printf("%d ",a[k]);
	}
	for (k = 0; k < i; k++)
	{
		if (a[k] > 0)
			printf("%d ",a[k]);
	}
}



