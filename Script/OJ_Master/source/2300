
//Problem2300


#include<stdio.h>
int main()
{
	int t, c, j;
	int i = 0;
	int a[100000];
	t = c = j = 0;
	while (scanf("%d",&a[i]),1)
	{
		if (a[i] <= 0)
		break;
		
		if (a[i] >= 85)
			t++;
		else if (a[i] < 60)
			j++;
		else
			c++;
		i++;

	}
	printf(">=85:%d\n", t);
	printf("60-84:%d\n", c);
	printf("<60:%d\n", j);

}



