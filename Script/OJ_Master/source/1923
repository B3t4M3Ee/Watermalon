
//Problem1923


#include<stdio.h>
int main()
{
	double a, c;
	char b;
	while (scanf("%lf%c%lf", &a, &b, &c)!=EOF)
	{
		scanf("%lf%c%lf", &a, &b, &c);
		if (b == '+')
			printf("%.2lf", a + c);
		else if (b == '-')
			printf("%.2lf", a - c);
		else if (b == '*')
			printf("%.2lf", a*c);
		else if (b == '/')
			printf("%.2lf",a / c);
		else
			printf("Unknown operator!");
	}
}



