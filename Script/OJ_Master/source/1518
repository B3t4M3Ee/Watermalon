
//Problem1518


#include<stdio.h>
int main()
{
	int n;
	while (scanf("%d",&n)!=EOF)
	{
		struct 
		{
			char num[100];
			char name[100];
			int a;
			int b;
			int c;
		}st[1000];
		for (int i = 0; i < n; i++)
		{
			scanf("%s%s%d%d%d", st[i].num, st[i].name, &st[i].a, &st[i].b, &st[i].c);
		}
		for (int i = 0; i < n; i++)
		{
			printf("%s,%s,%d,%d,%d\n", st[i].num, st[i].name, st[i].a, st[i].b, st[i].c);
		}
	}
}



