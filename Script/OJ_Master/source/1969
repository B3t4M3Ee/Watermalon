
//Problem1969


#include<stdio.h>
int main()
{
	int i;
	double min=10000, max=-1;
	double n,a[10];
	for (i = 0; i < 10; i++)
		scanf("%lf", &a[i]);
	for (i = 0; i < 10; i++)
	{
		if (a[i] > max)
			max=a[i];
		if (a[i] < min)
			min = a[i];
	}
	n = 0;
	for (i = 0; i < 10; i++)
		n = n + a[i];
	n = n - max - min;
	n = n / 8;
	printf("%.2lf", n);

}



