
//Problem1055




#include<stdio.h>
#include<string.h>
int main()
{
	char n[100];
	while (gets(n) != NULL)
	{
		int i;
		for (i = 0; i < strlen(n); i++)
		{
			if (n[i] >= 'a'&&n[i] <= 'c')
			{
				printf("2");
			}
			else if (n[i] >= 'd'&&n[i] <= 'f')
			{
				printf("3");

			}
			else if (n[i] >= 'g'&&n[i] <= 'i')
			{
				printf("4");
			}
			else if (n[i] >= 'j'&&n[i] <= 'l')
			{
				printf("5");
			}
			else if (n[i] >= 'm'&&n[i] <= 'o')
			{
				printf("6");
			}
			else if (n[i] >= 'p'&&n[i] <= 's')
			{
				printf("7");
			}
			else if (n[i] >= 't'&&n[i] <= 'v')
			{
				printf("8");
			}
			else if (n[i] >= 'w'&&n[i] <= 'z')
			{
				printf("9");
			}
			else if (n[i] >= 'A'&&n[i] <= 'Y')
			{
				printf("%c", n[i] + ('a' - 'A') + 1);
			}
			else if (n[i] == 'Z')
			{
				printf("a");
			}
			else
				printf("%c", n[i]);
			
		}printf("\n");
	}
}





