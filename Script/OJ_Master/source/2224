
//Problem2224


#include<stdio.h>
int main()
{
	int t;
	int i;
	int n, m, n1, m1;
	int q, w, e, r;
	while (scanf("%d/%d+%d/%d", &q, &w, &e, &r) != EOF)
	{
		n = q*r + w*e;
		m = w*r;
		for (i = 1; i <= m; i++)
		{
			if (m%i == 0 && n%i == 0)
			{
				t = i;

			}
		}
		if (m / t == 1) printf("%d\n", n / t);
		else
			printf("%d/%d\n", n / t, m / t);

	}
}



