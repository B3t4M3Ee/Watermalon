#include<stdio.h>
#include<string.h>
int main()
{
	int h;
	int t;
	int i; char n[1001];
	for (i = 0; i < 1001; i++)
	{
		n[i] = '\0';
	}
	while (gets(n)!=NULL)
	{
		int o = 0;
		h = strlen(n);
		for (i = 0; i < h; i++)
		{
				if (n[i] == 'w'&&n[i + 1] == 'e')
				{
					printf("you");
					i++;
				}
				else
				{
					printf("%c", n[i]);
				}
		}
		printf("\n");
		for (i = 0; i < 1001; i++)
		{
			n[i] = '\0';
		}
	}
	
}



