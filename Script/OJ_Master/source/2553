
//Problem2553


#include<string.h>
#include<stdio.h>
int main()
{
	char a[1000];
	gets(a);
	int n = strlen(a);
	int flag = 0;
	for (int i = 0; i < n; i++)
	{
		if (a[i] >= '0'&&a[i] <= '9' || a[i] >= 'a'&&a[i] <= 'z')
		{
			printf("%c", a[i]);
			flag = 0;
		}
		else
		{
			if (flag == 0)
			{
				printf("\n");
				flag = 1;
			}
		}
	}
}
