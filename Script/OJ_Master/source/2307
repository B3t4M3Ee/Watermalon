
//Problem2307


#include<stdio.h>
int main()
{
	int n;
	while (scanf("%d", &n) != EOF) {
		int a[16][16]; int k, i;
		for (k = 0; k<n; k++)
		{

			a[k][k] = 1;
			a[k][0] = 1;

		}
		for (k = 2; k<n; k++)
		{
			for (i = 1; i <= k - 1; i++)
			{
				a[k][i] = a[k - 1][i] + a[k - 1][i - 1];
			}
		}

		for (k = 0; k<n; k++)
		{
			for (i = 0; i<k; i++)
			{
				printf("%d ", a[k][i]);
			}printf("%d\n", a[k][i]);
		}
	}

}



