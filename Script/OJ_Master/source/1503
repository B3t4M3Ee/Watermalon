
//Problem1503


#include<stdio.h>
#include<string.h>
int main()
{
	int a=0, b=0, c=0, d=0;
	int i, m;
	char n[400];
	gets(n);
	m = strlen(n);
	for (i = 0; i < m; i++)
	{
		if (n[i] <= 'z'&&n[i] >= 'a')
			a = a + 1;
		else if (n[i] >= 'A'&&n[i] <= 'Z')
			a = a + 1;
		else if (n[i] >= '0'&&n[i] <= '9')
			b = b + 1;
		else if (n[i] == ' ')
			c = c + 1;

		else
			d = d + 1;


			
	} 

	printf("%d %d %d %d ", a, b, c, d);
}



