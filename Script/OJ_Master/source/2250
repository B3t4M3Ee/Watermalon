
//Problem2250


#include<stdio.h>
int main()
{
	int n;
	scanf("%d", &n);
	for (int i = 0; i < n; i++)
	{
		int t;
		scanf("%d", &t);
		for (int k = 0; k < (t + 1) / 2; k++)
		{
			for (int j = 0; j <(t+1)/2-k-1; j++)
			{
				printf(".");
			}
			for (int j = 0; j < 2 * (k+1) - 1; j++)
			{
				printf("*");
			}
			for (int j = 0; j < (t + 1) / 2 - k-1; j++)
			{
				printf(".");
			}
			printf("\n");
		}
	}
}



