
//Problem1171




#include<stdio.h>
#include<string.h>
int main()
{
	int i;

	char n[80];
	gets(n);
	for (i = 0; i < strlen(n); i++)
	{
		if (n[i] >= 'a'&&n[i] <= 'z')
			n[i] = n[i] - 32;
		else if (n[i] <= 'Z'&&n[i] >= 'A')

		{
			n[i] = n[i] + 32;

		}

	}
	for (i = 0; i < strlen(n); i++)
		printf("%c", n[i]);
}





