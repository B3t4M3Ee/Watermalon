
//Problem1091




#include <stdio.h>
int main() {
	int c;
	while ((c = getchar()) != EOF) {
		if (c == '<') {
			printf("&lt;");
		}
		else if (c == '>') {
			printf("&gt;");
		}
		else if (c == ' ') {
			printf("&nbsp;");
		}
		else if (c == '\t') {
			printf("&#9;");
		}
		else {
			printf("%c", c);
		}
	}
	return 0;
}





