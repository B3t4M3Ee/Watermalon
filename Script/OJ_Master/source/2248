
//Problem2248


#include<stdio.h>
int main()
{
	int n;
	scanf("%d", &n);
	for (int i = 0; i < n; i++)
	{
		int a, b;
		scanf("%d%d", &a, &b);
		for (int i = 0; i < a; i++)
		{
			printf("+");
			
		}printf("\n");
		for (int i = 0; i < b - 2; i++)
		{
			printf("+");
			for (int i = 0; i < a-2; i++)
				printf(" ");
			printf("+");
			printf("\n");
		}
		for (int i = 0; i < a; i++)
			printf("+");
		printf("\n");
	}
}



