
//Problem1432


#include<stdio.h>
#include<string.h>
int main()
{
	int i,a=0,b=0,c=0,d=0;
	char n[100000];
	gets(n);
	for (i = 0; i < strlen(n); i++)
	{
		if (n[i] >= 'a'&&n[i] <= 'z' || n[i] >= 'A'&&n[i] <= 'Z')
			a = a + 1;
		else if (n[i] >= '0'&& n[i] <= '9')
			b = b + 1;
		else if (n[i] == ' ')
		c = c+1;
		else
		d = d + 1;	
	}
	printf("%d %d %d %d", a, b, c, d);

}




