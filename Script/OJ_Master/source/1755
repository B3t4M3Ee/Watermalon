
//Problem1755


#include<stdio.h>
int main()
{
	int a[1000];
	int i = 0, t = 0,k=0;
	int b[1000];
	int c[1000];
	while (scanf("%d",&a[i]),a[i])
	{
		if (a[i] > 0)
		{
			b[t] = a[i];
			t++;
		}
		else
		{
			c[k] = a[i];
			k++;
		}
		i++;
	}
	for (int i = 0; i < t; i++)
	{
		printf("%d ", b[i]);
	}
	for (int i = 0; i < k; i++)
	{
		printf("%d ", c[i]);
	}
}



