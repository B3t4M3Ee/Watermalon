
//Problem2115


#include<stdio.h>
#include<string.h>
int main()
{
	int n;
	while (scanf("%d", &n) != EOF)
	{
		for (int i = 0; i < n; i++)
		{
			int t;
			scanf("%d", &t);
			char a[1000];
			scanf("%s", a);
			for (int k = t - 1; k < strlen(a); k++)
			{
				a[k] = a[k + 1];
			}
			printf("%d %s\n",i+1,a);
		}
	}
}



