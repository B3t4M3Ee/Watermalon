
//Problem2474


#include<stdio.h>
#include<string.h>
int main()
{
	char str1[1000];
	char str2[1000];
	scanf("%s%s", str1, str2);
	char *p;
	int n = strlen(str1);
	int m = strlen(str2);
	
	for (p = str1 + n - 1; p >= str1; p--)
	{
		printf("%c", *p);
	}
	for (p = str2 + m - 1; p >= str2; p--)
	{
		printf("%c", *p);
	}
}



